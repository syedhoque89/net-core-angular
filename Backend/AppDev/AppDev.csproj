      <DocumentationFile>bin\Debug\net5.0\AppDev.xml</DocumentationFile>
    </PropertyGroup>
    <ItemGroup>
      <PackageReference Include="Swashbuckle.AspNetCore.Swagger" Version="6.2.3" />
